RewriteEngine on
RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f

Rewriterule ^base/(.+)/(.+)/(.+)$ app/cont/principal/base.php?sec=$1&opcion=$2&id=$3
Rewriterule ^base/(.+)/(.+)$ app/cont/principal/base.php?sec=$1&opcion=$2
Rewriterule ^base/(.+) app/cont/principal/base.php?sec=$1

Rewriterule ^index.php$ https://admision.unm.edu.pe/docentesv1/app/cont/sesion/sesion.php
#Rewriterule ^index.php$ http://localhost/planilla/base/plancrd